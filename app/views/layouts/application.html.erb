<!DOCTYPE html>
<html>
  <head>
    <title>Dieggotv</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="nav">
      <div class="logo">
        <%= link_to image_tag("logo.svg"), root_path %>
        <%#= image_tag "logo.svg" %>
      </div>
      <div class="links">
        <%= link_to "Customers", customers_path %>
        <%= link_to "Packages", packages_path %>
        <%= link_to "Additional Services", additional_services_path %>
      </div>
    </div>

    <%= yield %>
  </body>
</html>
